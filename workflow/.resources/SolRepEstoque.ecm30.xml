var SeqConcluiMov = 24;&#xd;
var SeqGestorGSP = 55;&#xd;
&#xd;
function beforeStateEntry(sequenceId){&#xd;
&#xd;
	log.info(&quot;beforeStateEntry &quot;+sequenceId);&#xd;
	&#xd;
	//Define Respons?vel&#xd;
    if (sequenceId == SeqAtualizaWf) {&#xd;

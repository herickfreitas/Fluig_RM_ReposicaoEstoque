		log.info(&quot;cUser: &quot;+getValue(&quot;WKUser&quot;));&#xd;
		var constraintsEmail = new Array(cCompany, cUser);&#xd;
		var colleague = DatasetFactory.getDataset(&quot;colleague&quot;, null, constraintsEmail, null);&#xd;
		&#xd;
		var Email = colleague.getValue(0, &quot;mail&quot;);&#xd;
